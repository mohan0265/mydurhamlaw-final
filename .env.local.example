NEXT_PUBLIC_ENABLE_VOICE_FEATURES=false
NEXT_PUBLIC_ENABLE_PODCAST_FEATURES=false
NEXT_PUBLIC_ENABLE_WELLBEING_FEATURES=false

# Development
NEXT_TELEMETRY_DISABLED=1
NODE_ENV=development