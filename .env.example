# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Application Configuration
NEXT_PUBLIC_APP_URL=http://localhost:3000
NEXT_PUBLIC_APP_NAME=MyDurhamLaw AI Study App

# AI Services (Optional - features will be disabled if not provided)
OPENAI_API_KEY=your_openai_api_key
ELEVENLABS_API_KEY=your_elevenlabs_api_key

# Voice Configuration (Optional)
ELEVENLABS_VOICE_ID=your_preferred_voice_id
ELEVENLABS_MODEL=eleven_turbo_v2

# Always With You (AWY) Configuration
NEXT_PUBLIC_AWY_API_BASE=/api

# Security (Optional)
NEXTAUTH_SECRET=your_nextauth_secret
NEXTAUTH_URL=http://localhost:3000

# Email Services (Optional)
RESEND_API_KEY=your_resend_api_key

# Feature Flags
NEXT_PUBLIC_ENABLE_AI_FEATURES=false
NEXT_PUBLIC_ENABLE_VOICE_FEATURES=false
NEXT_PUBLIC_ENABLE_PODCAST_FEATURES=false
NEXT_PUBLIC_ENABLE_WELLBEING_FEATURES=false

# Development
NEXT_TELEMETRY_DISABLED=1
NODE_ENV=development