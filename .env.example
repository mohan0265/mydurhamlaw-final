# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Application Configuration
NEXT_PUBLIC_APP_URL=https://mydurhamlaw.com
NEXT_PUBLIC_APP_NAME=MyDurhamLaw AI Study App

# AI Services (Optional - features will be disabled if not provided)
OPENAI_API_KEY=your_openai_api_key
OPENAI_REALTIME_MODEL=gpt-4o-realtime-preview-2024-12-17

# Always With You (AWY) Configuration
NEXT_PUBLIC_AWY_API_BASE=/api

# Security (Optional)
NEXTAUTH_SECRET=your_nextauth_secret
NEXTAUTH_URL=https://mydurhamlaw.com

# Email Services (Optional)
RESEND_API_KEY=your_resend_api_key

# Feature Flags
NEXT_PUBLIC_ENABLE_AI_FEATURES=false
NEXT_PUBLIC_ENABLE_VOICE_FEATURES=false
NEXT_PUBLIC_ENABLE_PODCAST_FEATURES=false
NEXT_PUBLIC_ENABLE_WELLBEING_FEATURES=false
NEXT_PUBLIC_DURMAH_REALTIME_DEBUG=false

# Development
NEXT_TELEMETRY_DISABLED=1
NODE_ENV=development
